@article{MZI+03,
	author = {Pascal Merc\`{e}re and Philippe Zeitoun and Mourad Idir and S\'{e}bastien Le Pape and Denis Douillet and Xavier Levecq and Guillaume Dovillaire and Samuel Bucourt and Kenneth A. Goldberg and Patrick P. Naulleau and Senajith Rekawa},
	journal = {Opt. Lett.},
	keywords = {Metrology; Phase measurement; Surface measurements, figure; Lithography; Ultraviolet, extreme},
	number = {17},
	pages = {1534--1536},
	publisher = {OSA},
	title = {Hartmann wave-front measurement at 13.4 nm with $\lambda$EUV/120 accuracy},
	volume = {28},
	month = {9},
	year = {2003},
	url = {http://ol.osa.org/abstract.cfm?URI=ol-28-17-1534},
	doi = {10.1364/OL.28.001534},
	abstract = {We report, for the first time to our knowledge, experimental demonstration of wave-front analysis via the Hartmann technique in the extreme ultraviolet range. The reference wave front needed to calibrate the sensor was generated by spatially filtering a focused undulator beam with 1.7- and 0.6-$\mu$m -diameter pinholes. To fully characterize the sensor, accuracy and sensitivity measurements were performed. The incident beam's wavelength was varied from 7 to 25 nm. Measurements of accuracy better than $\lambda$EUV/120 (0.11 nm) were obtained at $\lambda$EUV$=$13.4 nm . The aberrations introduced by an additional thin mirror, as well as wave front of the spatially unfiltered incident beam, were also measured.},
}

